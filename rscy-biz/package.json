{
  "name": "rscy-biz",
  "version": "0.0.1",
  "description": "rscy shit",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon ./src/rscy-world.ts",
    "build:gates": "vite build",
    "build:world:serv": "tsc src/rscy-world.ts --outDir dist --esModuleInterop",
    "build:world:deps": "cp package.json dist/package.json && cp package-lock.json dist/package-lock.json && cd dist && npm ci --production",
    "build:world": "npm run build:world:serv && npm run build:world:deps",
    "start": "NODE_ENV=prod node dist/rscy-world.js"
  },
  "author": "Mike Pottebaum",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.5"
  }
}
